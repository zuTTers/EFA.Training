import { ElementRef, EventEmitter, OnDestroy, OnInit } from '@angular/core';
import { BehaviorSubject, AsyncSubject } from 'rxjs';
import { ScannerQRCodeConfig, ScannerQRCodeDevice, ScannerQRCodeResult, ScannerQRCodeSelectedFiles } from './ngx-scanner-qrcode.options';
export declare class NgxScannerQrcodeComponent implements OnInit, OnDestroy {
    /**
     * Element
     * playsinline required to tell iOS safari we don't want fullscreen
     */
    video: ElementRef<HTMLVideoElement>;
    canvas: ElementRef<HTMLCanvasElement>;
    resultsPanel: ElementRef<HTMLDivElement>;
    /**
     * EventEmitter
     */
    event: EventEmitter<ScannerQRCodeResult[]>;
    /**
     * Input
     */
    src: string | undefined;
    fps: number | undefined;
    vibrate: number | undefined;
    decode: string | undefined;
    isBeep: boolean | undefined;
    deviceActive: number | undefined;
    config: ScannerQRCodeConfig;
    constraints: MediaStreamConstraints | any | undefined;
    /**
     * Export
     */
    isStart: boolean;
    isLoading: boolean;
    isTorch: boolean;
    data: BehaviorSubject<ScannerQRCodeResult[]>;
    devices: BehaviorSubject<ScannerQRCodeDevice[]>;
    /**
     * Private
     */
    private rAF_ID;
    private dataForResize;
    ngOnInit(): void;
    /**
     * start
     * @return AsyncSubject
     */
    start(): AsyncSubject<any>;
    /**
     * playDevice
     * @param deviceId
     * @param as
     * @return AsyncSubject
     */
    playDevice(deviceId: string, as?: AsyncSubject<any>): AsyncSubject<any>;
    /**
     * stop
     * @return AsyncSubject
     */
    stop(): AsyncSubject<any>;
    /**
     * play
     * @return AsyncSubject
     */
    play(): AsyncSubject<any>;
    /**
     * pause
     * @return AsyncSubject
     */
    pause(): AsyncSubject<any>;
    /**
     * loadImage
     * @param src
     * @return AsyncSubject
     */
    loadImage(src: string): AsyncSubject<any>;
    /**
     * torcher
     * @return AsyncSubject
     */
    torcher(): AsyncSubject<any>;
    /**
     * applyConstraints
     * @return AsyncSubject
     */
    applyConstraints(constraints: MediaTrackConstraintSet | any): AsyncSubject<any>;
    /**
     * getConstraints
     * @param deviceActive
     * @returns
     */
    getConstraints(): MediaTrackConstraintSet | any;
    /**
     * download
     * @param fileName
     * @return AsyncSubject
     */
    download(fileName?: string): AsyncSubject<ScannerQRCodeSelectedFiles[]>;
    /**
     * window: resize
     * Draw again!
     */
    private resize;
    /**
     * Override config
     * @return void
     */
    private overrideConfig;
    /**
     * loadAllDevices
     */
    private loadAllDevices;
    /**
     * drawImage
     * @param element
     * @param callback
     */
    private drawImage;
    /**
     * removeCanvas
     */
    private removeCanvas;
    /**
     * status
     * @param isStart
     * @param isLoading
     */
    private status;
    /**
     * eventEmit
     * @param response
     */
    private eventEmit;
    /**
     * Single-thread
     * Loop Recording on Camera
     * Must be destroy request
     * Not using: requestAnimationFrame
     */
    private requestAnimationFrame;
    /**
     * Status of camera
     * @return boolean
     */
    readonly isPause: boolean;
    ngOnDestroy(): void;
}
