            var mergedOptions = this.smartMerge(options, this.themeService.getColorschemesOptions());
            /** @type {?} */

            var chartConfig = {
              type: this.chartType,
              data: {
                labels: this.labels || [],
                datasets: datasets
              },
              plugins: this.plugins,
              options: mergedOptions
            };
            return chartConfig;
          }
          /**
           * @param {?} ctx
           * @return {?}
