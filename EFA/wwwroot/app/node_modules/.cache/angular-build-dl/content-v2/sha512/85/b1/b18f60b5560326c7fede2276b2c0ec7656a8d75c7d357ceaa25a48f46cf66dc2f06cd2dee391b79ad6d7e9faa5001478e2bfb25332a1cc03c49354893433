t.previousElementSibling;\r\n        }\r\n        else if (keyCode === Keys.right) {\r\n            nextCellElement = cellElement.nextElementSibling;\r\n        }\r\n        else if (keyCode === Keys.up || keyCode === Keys.down) {\r\n            const nextRowElement = this.getPrevNextRow(rowElement, keyCode);\r\n            if (nextRowElement) {\r\n                const children = nextRowElement.getElementsByClassName('datatable-body-cell');\r\n                if (children.length)\r\n                    nextCellElement = children[cellIndex];\r\n            }\r\n        }\r\n        if (nextCellElement)\r\n            nextCellElement.focus();\r\n    }\r\n    getRowSelected(row) {\r\n        return this.getRowSelectedIdx(row, this.selected) > -1;\r\n    }\r\n    getRowSelectedIdx(row, selected) {\r\n        if (!selected || !selected.length)\r\n            return -1;\r\n        const rowId = this.rowIdentity(row);\r\n        return selected.findIndex(r => {\r\n            const id = this.rowIdentity(r);\r\n            return id === rowId;\r\n        });\r\n    }\r\n};\r\nDataTableSelectionComponent.fac = function DataTableSelectionComponent_Factory(t) { return new (t || DataTableSelectionComponent)(); };\r\nDataTableSelectionComponent.cmp = /*@__PURE__*/ ngcc0.defineComponent({ type: DataTableSelectionComponent, selectors: [[\"datatable-selection\"]], inputs: { rows: \"rows\", selected: \"selected\", selectEnabled: \"selectEnabled\", selectionType: \"selectionType\", rowIdentity: \"rowIdentity\", selectCheck: \"selectCheck\" }, outputs: { activate: \"activate\", select: \"select\" }, ngContentSelectors: _c0, decls: 1, vars: 0, template: function DataTableSelectionComponent_Template(rf, ctx) { if (rf & 1) {\r\n        ngcc0.projectionDef();\r\n        ngcc0.projection(0);\r\n    } }, encapsulation: 2, changeDetection: 0 });\r\n__decorate([\r\n    Input()\r\n], DataTableSelectionComponent.prototype, \"rows\", void 0);\r\n__decorate([\r\n    Input()\r\n], DataTableSelect